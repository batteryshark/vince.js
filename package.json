{
  "name": "vince.js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next .swc",
    "clean:all": "npm run clean && rm -f prisma/dev.db prisma/dev.db-journal",
    "clean:db": "rm -f prisma/dev.db prisma/dev.db-journal && echo 'Database cleaned. Run npm run db:migrate:dev before next startup.'",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:dev": "./scripts/dev-migrate.sh",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "./scripts/dev-prisma.sh studio",
    "db:studio:dev": "./scripts/dev-prisma.sh studio",
    "db:reset": "prisma migrate reset",
    "db:reset:dev": "./scripts/dev-prisma.sh migrate reset",
    "db:setup": "node scripts/setup-database.js && prisma generate && prisma db push",
    "db:setup:prod": "node scripts/setup-database.js && prisma generate && prisma migrate deploy",
    "config:validate": "node scripts/validate-config.js",
    "config:setup": "node scripts/validate-config.js && npm run db:setup",
    "secrets:generate": "node scripts/generate-secrets.js",
    "docker:setup": "npm run secrets:generate && echo '\nüê≥ Run: docker-compose up -d'"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.0",
    "jsonwebtoken": "^9.0.0",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.5.6",
    "prisma": "^6.14.0",
    "tailwindcss": "^4.1.12"
  }
}
